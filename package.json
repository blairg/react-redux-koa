{
  "name": "react-redux-koa",
  "version": "0.0.1",
  "description": "Hacking around with node, koa, react and redux.",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "webserver": "node api/src/server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "build": "node_modules/.bin/webpack",
    "dev": "node_modules/.bin/webpack-dev-server",
    "lint": "eslint . --ext .js --ext .jsx",
    "deploy": "NODE_ENV=production webpack -p",
    "clean": "rm -rf app/dist app/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blairg/react-redux-koa.git"
  },
  "keywords": [
    "boilerplate",
    "koa",
    "redux",
    "react",
    "webpack",
    "mocha",
    "sass"
  ],
  "author": "Blair Garrett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/blairg/react-redux-koa/issues"
  },
  "homepage": "https://github.com/blairg/react-redux-koa#readme",
  "dependencies": {
    "bookshelf": "^0.10.0",
    "css-loader": "^0.26.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.11.10",
    "koa": "^1.2.1",
    "koa-cors": "0.0.16",
    "koa-route": "^2.4.2",
    "mysql": "^2.11.1",
    "node-sass": "^4.3.0",
    "react": "^15.3.2",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "eslint": "^3.14.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.3.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  }
